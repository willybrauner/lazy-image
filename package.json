{
  "name": "@wbe/lazy-image",
  "version": "3.1.1-0",
  "description": "Lazy and responsive image management for image and background-image",
  "author": "Willy Brauner",
  "license": "MIT",
  "types": "dist/index",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "private": false,
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/willybrauner/lazy-image.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "microbundle --jsx React.createElement",
    "dev:vanilla": "vite --config vite.config.vanilla.ts",
    "dev:react": "vite --config vite.config.react.ts",
    "pre-publish": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@wbe/fake-data-utils": "^2.3.0",
    "microbundle": "^0.15.0",
    "prettier": "^2.6.2",
    "vite": "^2.9.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "keywords": [
    "lazy",
    "image",
    "background-image",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  }
}
